<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".MainActivity">

		<RelativeLayout android:id="@+id/content_container"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" >

				<View
					android:id="@+id/rectangle_4"
					android:layout_width="match_parent"
					android:layout_height="360dp"
					android:layout_marginStart="0dp"
					android:layout_marginTop="370dp"
					android:background="@drawable/rectangle_4_shape" />

				<View
					android:id="@+id/frame"
					android:layout_width="460dp"
					android:layout_height="409dp"
					android:layout_marginStart="-8dp"
					android:layout_marginTop="370dp"
					android:background="@drawable/frame_shape" />

			<ImageView
				android:id="@+id/foto1"
				android:layout_width="match_parent"
				android:layout_height="373dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:contentDescription="foto1"
				android:src="@drawable/foto1" />


				<ImageView
					android:id="@+id/foto2"
					android:layout_width="300dp"
					android:layout_height="140dp"
					android:layout_marginLeft="60dp"
					android:layout_marginTop="380dp"
					android:src="@drawable/foto2" />

				<RelativeLayout
					android:id="@+id/frame_ek1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="8dp"
					android:layout_marginTop="559dp">

					<ImageButton
						android:id="@+id/btn_start"
						android:layout_width="345dp"
						android:layout_height="62dp"
						android:layout_marginStart="30dp"
						android:layout_marginTop="17dp"
						android:background="@drawable/_bg__frame_ek2_shape" />

					<TextView
						android:id="@+id/get_started"
						android:layout_width="101dp"
						android:layout_height="24.6dp"
						android:layout_gravity="center_horizontal|center"
						android:layout_marginLeft="155dp"
						android:layout_marginTop="40dp"
						android:gravity="center"
						android:text="@string/get_started_string"
						android:textColor="@color/get_started_color"
						android:textSize="16sp"
						android:textStyle="bold" />

				</RelativeLayout>

			<TextView
				android:id="@+id/the_animated_rick_and_morty_characters_originate_from_america_created_by_justin_roiland_and_dan_harmon_container"
				android:layout_width="341.4dp"
				android:layout_height="55.4dp"
				android:layout_gravity="center_horizontal|center"
				android:layout_marginLeft="40dp"
				android:layout_marginTop="500dp"
				android:gravity="center"
				android:text="@string/the_animated_rick_and_morty_characters_originate_from_america_created_by_justin_roiland_and_dan_harmon_container_string"
				android:textColor="@color/the_animated_rick_and_morty_characters_originate_from_america_created_by_justin_roiland_and_dan_harmon_container_color"
				android:textSize="14sp"
				android:textStyle="bold" />

			</RelativeLayout>
</RelativeLayout>